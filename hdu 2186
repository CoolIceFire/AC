# include <stdio.h>
int main()
{
    int n,m,a,b,c,s;
    scanf("%d",&n);
    while(n--)
    {
        s=0;
        scanf("%d",&m);
        a=m/2,b=2*(m-a)/3,c=m-a-b;
        if(a%10)s=a/10+1;
        else s=a/10;
        if(b%10)s+=(b/10+1);
        else s+=b/10;
        if(c%10)s+=(c/10+1);
        else s+=c/10;
        printf("%d\n",s);
    }
    return 0;
}
