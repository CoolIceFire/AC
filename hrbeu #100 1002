#include<iostream>
#include<algorithm>

using namespace std;

int num[5010];

int main()
{
        int n,m;
        char str[10];
        scanf("%d",&n);
        for(int i=1;i<=n;i++)
                scanf("%d",&num[i]);
        scanf("%s",str);
        sort(num+1,num+n+1);
        scanf("%d",&m);
        for(int j,i=1;i<=m;i++)
        {
                scanf("%d",&j);
                printf("%d\n",num[j]);
        }
}
