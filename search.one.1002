#include<stdio.h>
#include<string.h>
char ch[110][110];
int h,w;
int dfs(int x,int y)
{
  int i=x,j=y;
	if(i>0&&ch[i-1][j]=='#')ch[i-1][j]='!',dfs(i-1,j);
	if(i<h-1&&ch[i+1][j]=='#')ch[i+1][j]='!',dfs(i+1,j);
	if(j>0&&ch[i][j-1]=='#')ch[i][j-1]='!',dfs(i,j-1);
	if(j<w-1&&ch[i][j+1]=='#')ch[i][j+1]='!',dfs(i,j+1);
	return 0;
}
int main()
{
	int T,i,j,ans;
	scanf("%d",&T);
	while(T--)
	{
		scanf("%d%d",&h,&w);
		getchar();
		for(i=0;i<110;i++)
			for(j=0;j<110;j++)ch[i][j]='*';
		for(i=0;i<h;i++)
		{
			for(j=0;j<w;j++)scanf("%c",&ch[i][j]);
			getchar();
		}
		for(i=0;i<h;i++)
		{
			for(j=0;j<w;j++)
			{
				if(ch[i][j]=='#')
				{
					dfs(i,j);
					ch[i][j]='#';
				}
			}
		}
		for(ans=i=0;i<h;i++)
		{
			for(j=0;j<w;j++)
				if(ch[i][j]=='#')ans+=1;
		}
		printf("%d\n",ans);
	}
	return 0;
}
