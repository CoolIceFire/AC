#include<stdio.h>
int main()
{
    int a,b,c,n,i;
    scanf("%d",&n);
    i=1;
    while(n--)
    {
        scanf("%d%d%d",&a,&b,&c);
        if(a*a+b*b==c*c||a*a+c*c==b*b||b*b+c*c==a*a)
            printf("Scenario #%d:\nyes\n",i);
        else printf("Scenario #%d:\nno\n",i);
        puts("");
        i+=1;
    }
    return 0;
}
